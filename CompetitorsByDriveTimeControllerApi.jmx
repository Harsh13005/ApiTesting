          <stringProp name="script">import org.apache.jmeter.engine.StandardJMeterEngine
import org.apache.jmeter.threads.ThreadGroup
import org.apache.jorphan.collections.HashTree
import org.apache.jorphan.collections.SearchByClass
import java.lang.reflect.Field

import groovy.json.JsonSlurper
import groovy.json.JsonOutput
import java.util.Map


// Below code block reads all thread group names and sets following default properties
// 1. tph, 2. threads 3. iterations

String[] strThreadGroupName;

String strType;
String strName;
String strPropertyName;
String strPropertyValue;


StandardJMeterEngine engine = ctx.getEngine()
Field test = engine.getClass().getDeclaredField(&quot;test&quot;)
test.setAccessible(true)
HashTree testPlanTree = (HashTree) test.get(engine)

SearchByClass&lt;ThreadGroup&gt; threadGroupSearch = new SearchByClass&lt;&gt;(ThreadGroup.class)
testPlanTree.traverse(threadGroupSearch)
Collection&lt;ThreadGroup&gt; threadGroups = threadGroupSearch.getSearchResults()
threadGroups.each { 
    log.info(&quot;Thread Group: &quot; + it.getName() + &quot;; Number of threads: &quot; + it.getNumThreads())
    strThreadGroupName = it.getName().split(&quot;:&quot;);
    if(strThreadGroupName.size()==3)
    {
		log.info(strThreadGroupName[0]);
		log.info(strThreadGroupName[1]);
		log.info(strThreadGroupName[2]);
		//Trim spaces and convert to lower case
		strType = strThreadGroupName[1].trim().toLowerCase();
		strName = strThreadGroupName[2].trim().toLowerCase();
		if((strType==&quot;endpoint&quot;) || (strType==&quot;flow&quot;))
		{
			props.put(strType + &quot;.&quot; + strName + &quot;.&quot; + &quot;tph&quot;, 1);
			props.put(strType + &quot;.&quot; + strName + &quot;.&quot; + &quot;threads&quot;, 1);
			props.put(strType + &quot;.&quot; + strName + &quot;.&quot; + &quot;iterations&quot;, 1);	
		}

    } //end if strThreadGroupName
    else
    {
    	log.info(&quot;Note: Thread Group Name &quot; + it.getName() + &quot; need to be of format Thread Group : Endpoint / Flow / Custom : &lt;Name&gt;. Since the format is wrong custom code not executed to set custom property values&quot;);
    }
    
} 

//Below code reads the taurus yml properties and sets them as jmeter properties

log.info(&quot;testing&quot;);
//log.info(vars.get(&quot;tph_getAdultBeveragesSalesForWeekUsingGET&quot;));
//log.info(props.get(&quot;getAdultBeveragesSalesForWeekUsingGET.tph&quot;));
//log.info((new JsonSlurper()).parseText(props.get(&quot;getAdultBeveragesSalesForWeekUsingGET&quot;)).x);
//log.info(props.get(&quot;getAdultBeveragesSalesForWeekUsingGET&quot;).replaceAll(&quot;\&apos;&quot;, &quot;\&quot;&quot;));

def jsonSlurper = new JsonSlurper();
def response;
String strPropertyRoot = &quot;endpoint&quot;;
if(props.get(strPropertyRoot)!=null)
{
	response = jsonSlurper.parseText(props.get(strPropertyRoot).replaceAll(&quot;\&apos;&quot;, &quot;\&quot;&quot;));
	//def json = JsonOutput.toJson(response.getAdultBeveragesSalesForWeekUsingGET.tph);
	//vars.put(&quot;json&quot;, json);
	//log.info(json);
	//def map = (new groovy.json.JsonSlurper()).parse(props.get(&quot;endpoint&quot;).replaceAll(&quot;\&apos;&quot;, &quot;\&quot;&quot;));
	//map.each { key, value -&gt; println &quot;$key : $value&quot;}
	for (Map.Entry entry1 : response.entrySet()) 
	{
	    log.info(entry1.getKey() + &quot;  ==&gt;&gt;  &quot; + entry1.getValue().toString() + &quot;\n&quot;);
	    for (Map.Entry entry2 : entry1.getValue().entrySet())
	    {
	    	log.info(entry2.getKey());
	    	strPropertyName=strPropertyRoot + &quot;.&quot; + entry1.getKey() + &quot;.&quot; + entry2.getKey();
	    	props.put(strPropertyName.toString().trim().toLowerCase(), entry2.getValue().toString().trim().toLowerCase());
	    }
	    //log.info(entry.getValue().tph);	    
	}
}

//Below code logs all the jmeter properties
def jMeterProps;
jMeterProps = &quot;Quantity of properties: &quot; + props.entrySet().size() + &quot;.\n\n&quot;;
jMeterProps += &quot;[PROPERTY NAME]      ==&gt;&gt;      [PROPERTY VALUE]\n\n&quot;;
for (Map.Entry entry : props.entrySet()) 
{
    jMeterProps += entry.getKey() + &quot;  ==&gt;&gt;  &quot; + entry.getValue().toString() + &quot;\n&quot;;
}

log.info(jMeterProps);

//Below code logs all the jmeter variables
def jMeterVars;
jMeterVars = &quot;Quantity of variables: &quot; + vars.entrySet().size() + &quot;.\n\n&quot;;
jMeterVars += &quot;[VARIABLE NAME]      ==&gt;&gt;      [VARIABLE VALUE]\n\n&quot;;
for (Map.Entry entry : vars.entrySet()) {
    jMeterVars += entry.getKey() + &quot;  ==&gt;&gt;  &quot; + entry.getValue().toString() + &quot;\n&quot;;
}

log.info(jMeterVars);

log.info(&quot;End of script&quot;);
